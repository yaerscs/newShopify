    { "type": "range", "id": "row_min_height_desktop", "min": 40, "max": 140, "step": 2, "unit": "px", "label": "Row min height (desktop)", "default": 86 },
    { "type": "range", "id": "row_min_height_mobile", "min": 40, "max": 140, "step": 2, "unit": "px", "label": "Row min height (mobile)", "default": 100 },
    { "type": "range", "id": "product_image_height_desktop", "min": 40, "max": 220, "step": 2, "unit": "px", "label": "Product image max height (desktop)", "default": 106 },
    { "type": "range", "id": "product_image_height_mobile", "min": 40, "max": 220, "step": 2, "unit": "px", "label": "Product image max height (mobile)", "default": 120 },
    { "type": "range", "id": "product_image_overlap", "min": 0, "max": 140, "step": 2, "unit": "px", "label": "Product image overlap (desktop)", "default": 86 },
    { "type": "range", "id": "product_image_overlap_mobile", "min": 0, "max": 140, "step": 2, "unit": "px", "label": "Product image overlap (mobile)", "default": 80 },
