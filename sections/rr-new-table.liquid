    { "type": "range", "id": "column_radius", "min": 0, "max": 24, "step": 1, "unit": "px", "label": "Column corner radius", "default": 6 },

    { "type": "range", "id": "heading_size_desktop", "min": 20, "max": 64, "step": 1, "unit": "px", "label": "Heading size (desktop)", "default": 52 },
    { "type": "range", "id": "heading_size_mobile", "min": 18, "max": 44, "step": 1, "unit": "px", "label": "Heading size (mobile)", "default": 34 },
