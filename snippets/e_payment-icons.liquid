{% comment %}
  Payment Icons Snippet with Custom SVG and Image Override Support
  
  Usage:
  {% render 'e_payment-icons', block: block %}
  
  Block-level settings take precedence over global settings
{% endcomment %}

{% assign use_block_custom = false %}
{% assign block_settings = false %}

{% if block %}
  {% assign block_settings = block.settings %}
  {% if block.settings.use_custom_icons %}
    {% assign use_block_custom = true %}
  {% endif %}
{% endif %}

<div class="payment-icons" style="
  {% if block_settings %}
    text-align: {{ block_settings.alignment | default: 'center' }};
  {% endif %}
">
  {% if use_block_custom and block_settings.payment_override_image %}
    {%- comment -%} Use block-level override image {%- endcomment -%}
    <div class="payment-icons__wrapper payment-icons__override" style="
      {% if block_settings %}
        margin-top: {{ block_settings.margin_top | default: 8 }}px;
        margin-bottom: {{ block_settings.margin_bottom | default: 8 }}px;
      {% endif %}
    ">
      <img 
        src="{{ block_settings.payment_override_image | image_url: width: 200 }}" 
        alt="Payment methods accepted"
        width="200"
        height="{{ block_settings.icon_height | default: 22 }}"
        class="payment-override-image"
        loading="lazy"
      />
    </div>
  
  {% elsif use_block_custom %}
    {%- comment -%} Use block-level custom SVGs {%- endcomment -%}
    <div class="payment-icons__wrapper" style="
      {% if block_settings %}
        gap: {{ block_settings.icon_spacing | default: 6 }}px;
      {% endif %}
    ">
      {% if block_settings.custom_svg_1 != blank %}
        <div class="payment-icon payment-icon--custom" style="height: {{ block_settings.icon_height | default: 22 }}px;">
          {{ block_settings.custom_svg_1 }}
        </div>
      {% endif %}
      
      {% if block_settings.custom_svg_2 != blank %}
        <div class="payment-icon payment-icon--custom" style="height: {{ block_settings.icon_height | default: 22 }}px;">
          {{ block_settings.custom_svg_2 }}
        </div>
      {% endif %}
      
      {% if block_settings.custom_svg_3 != blank %}
        <div class="payment-icon payment-icon--custom" style="height: {{ block_settings.icon_height | default: 22 }}px;">
          {{ block_settings.custom_svg_3 }}
        </div>
      {% endif %}
      
      {% if block_settings.custom_svg_4 != blank %}
        <div class="payment-icon payment-icon--custom" style="height: {{ block_settings.icon_height | default: 22 }}px;">
          {{ block_settings.custom_svg_4 }}
        </div>
      {% endif %}
      
      {% if block_settings.custom_svg_5 != blank %}
        <div class="payment-icon payment-icon--custom" style="height: {{ block_settings.icon_height | default: 22 }}px;">
          {{ block_settings.custom_svg_5 }}
        </div>
      {% endif %}
      
      {% if block_settings.custom_svg_6 != blank %}
        <div class="payment-icon payment-icon--custom" style="height: {{ block_settings.icon_height | default: 22 }}px;">
          {{ block_settings.custom_svg_6 }}
        </div>
      {% endif %}
    </div>

  {% elsif settings.payment_icons_use_custom and settings.payment_override_image %}
    {%- comment -%} Use global override image {%- endcomment -%}
    <div class="payment-icons__wrapper payment-icons__override" style="
      margin-top: {{ settings.payment_override_margin_top }}px;
      margin-bottom: {{ settings.payment_override_margin_bottom }}px;
      padding-top: {{ settings.payment_override_padding_top }}px;
      padding-bottom: {{ settings.payment_override_padding_bottom }}px;
      padding-left: {{ settings.payment_override_padding_left }}px;
      padding-right: {{ settings.payment_override_padding_right }}px;
    ">
      <img 
        src="{{ settings.payment_override_image | image_url: width: 200 }}" 
        alt="Payment methods accepted"
        width="200"
        height="40"
        class="payment-override-image"
        loading="lazy"
      />
    </div>
  
  {% elsif settings.payment_icons_use_custom %}
    {%- comment -%} Use global custom SVGs {%- endcomment -%}
    <div class="payment-icons__wrapper">
      {% if settings.payment_custom_svg_1 != blank %}
        <div class="payment-icon payment-icon--custom">
          {{ settings.payment_custom_svg_1 }}
        </div>
      {% endif %}
      
      {% if settings.payment_custom_svg_2 != blank %}
        <div class="payment-icon payment-icon--custom">
          {{ settings.payment_custom_svg_2 }}
        </div>
      {% endif %}
      
      {% if settings.payment_custom_svg_3 != blank %}
        <div class="payment-icon payment-icon--custom">
          {{ settings.payment_custom_svg_3 }}
        </div>
      {% endif %}
      
      {% if settings.payment_custom_svg_4 != blank %}
        <div class="payment-icon payment-icon--custom">
          {{ settings.payment_custom_svg_4 }}
        </div>
      {% endif %}
      
      {% if settings.payment_custom_svg_5 != blank %}
        <div class="payment-icon payment-icon--custom">
          {{ settings.payment_custom_svg_5 }}
        </div>
      {% endif %}
      
      {% if settings.payment_custom_svg_6 != blank %}
        <div class="payment-icon payment-icon--custom">
          {{ settings.payment_custom_svg_6 }}
        </div>
      {% endif %}
    </div>
  
  {% else %}
    {%- comment -%} Use default SVG icons {%- endcomment -%}
    <div class="payment-icons__wrapper" style="
      {% if block_settings %}
        gap: {{ block_settings.icon_spacing | default: 6 }}px;
